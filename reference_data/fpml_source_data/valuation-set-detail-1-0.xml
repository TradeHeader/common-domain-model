<?xml version="1.0" encoding="UTF-8"?>
<gcl:CodeList xmlns:gcl="http://xml.genericode.org/2004/ns/CodeList/0.2/" xmlns:doc="http://www.fpml.org/coding-scheme/documentation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xml.genericode.org/2004/ns/CodeList/0.2/ CodeList.xsd">
   <Annotation>
      <Description>
         <doc:definition>Specifies the amount of detail provided in the valuation set, e.g. is market environment data provided, are risk definitions provided, etc.</doc:definition>
      </Description>
   </Annotation>
   <Identification>
      <ShortName>valuationSetDetailScheme</ShortName>
      <Version>1-0</Version>
      <CanonicalUri>http://www.fpml.org/coding-scheme/valuation-set-detail</CanonicalUri>
      <CanonicalVersionUri>http://www.fpml.org/coding-scheme/valuation-set-detail-1-0</CanonicalVersionUri>
      <LocationUri>http://www.fpml.org/coding-scheme/valuation-set-detail-1-0.xml</LocationUri>
   </Identification>
   <ColumnSet>
      <Column Id="Code" Use="required">
         <ShortName>Code</ShortName>
         <Data Type="token"/>
      </Column>
      <Column Id="Source" Use="optional">
         <ShortName>Source</ShortName>
         <Data Type="string"/>
      </Column>
      <Column Id="Description" Use="optional">
         <ShortName>Description</ShortName>
         <Data Type="string"/>
      </Column>
      <Key Id="PrimaryKey">
         <ShortName>key</ShortName>
         <ColumnRef Ref="Code"/>
      </Key>
   </ColumnSet>
   <SimpleCodeList>
      <Row>
         <Value>
            <SimpleValue>Minimum</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Least applicable amount of data.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>Moderate</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Provide specific definition of each quotation with respect to market inputs.</SimpleValue>
         </Value>
      </Row>
      <Row>
         <Value>
            <SimpleValue>Full</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>FpML</SimpleValue>
         </Value>
         <Value>
            <SimpleValue>Provide full calculation details for each quotation.</SimpleValue>
         </Value>
      </Row>
   </SimpleCodeList>
</gcl:CodeList>